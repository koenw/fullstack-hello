#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+parser@7.16.4/node_modules/@babel/parser/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+parser@7.16.4/node_modules/@babel/parser/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+parser@7.16.4/node_modules/@babel/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+parser@7.16.4/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+parser@7.16.4/node_modules/@babel/parser/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+parser@7.16.4/node_modules/@babel/parser/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+parser@7.16.4/node_modules/@babel/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+parser@7.16.4/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../bin/babel-parser.js" "$@"
fi
