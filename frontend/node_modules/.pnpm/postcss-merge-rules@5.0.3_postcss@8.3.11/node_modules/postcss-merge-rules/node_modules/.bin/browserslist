#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/browserslist@4.18.1/node_modules/browserslist/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/browserslist@4.18.1/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/postcss-merge-rules@5.0.3_postcss@8.3.11/node_modules/postcss-merge-rules/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/postcss-merge-rules@5.0.3_postcss@8.3.11/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/browserslist@4.18.1/node_modules/browserslist/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/browserslist@4.18.1/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/postcss-merge-rules@5.0.3_postcss@8.3.11/node_modules/postcss-merge-rules/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/postcss-merge-rules@5.0.3_postcss@8.3.11/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.18.1/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.18.1/node_modules/browserslist/cli.js" "$@"
fi
