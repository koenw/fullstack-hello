#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/ejs@3.1.6/node_modules/ejs/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/ejs@3.1.6/node_modules/ejs/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/ejs@3.1.6/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+reporter-dev-server@2.0.1_@parcel+core@2.0.1/node_modules/@parcel/reporter-dev-server/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+reporter-dev-server@2.0.1_@parcel+core@2.0.1/node_modules/@parcel/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+reporter-dev-server@2.0.1_@parcel+core@2.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/ejs@3.1.6/node_modules/ejs/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/ejs@3.1.6/node_modules/ejs/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/ejs@3.1.6/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+reporter-dev-server@2.0.1_@parcel+core@2.0.1/node_modules/@parcel/reporter-dev-server/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+reporter-dev-server@2.0.1_@parcel+core@2.0.1/node_modules/@parcel/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+reporter-dev-server@2.0.1_@parcel+core@2.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ejs@3.1.6/node_modules/ejs/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../ejs@3.1.6/node_modules/ejs/bin/cli.js" "$@"
fi
