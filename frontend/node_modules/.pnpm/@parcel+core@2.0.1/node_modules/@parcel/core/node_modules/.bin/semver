#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@5.7.1/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+core@2.0.1/node_modules/@parcel/core/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+core@2.0.1/node_modules/@parcel/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+core@2.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@5.7.1/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+core@2.0.1/node_modules/@parcel/core/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+core@2.0.1/node_modules/@parcel/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@parcel+core@2.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
else
  exec node  "$basedir/../../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
fi
