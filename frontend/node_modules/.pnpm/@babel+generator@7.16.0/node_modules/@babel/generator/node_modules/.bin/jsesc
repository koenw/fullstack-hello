#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+generator@7.16.0/node_modules/@babel/generator/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+generator@7.16.0/node_modules/@babel/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+generator@7.16.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+generator@7.16.0/node_modules/@babel/generator/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+generator@7.16.0/node_modules/@babel/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/@babel+generator@7.16.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../jsesc@2.5.2/node_modules/jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../../../../../../jsesc@2.5.2/node_modules/jsesc/bin/jsesc" "$@"
fi
