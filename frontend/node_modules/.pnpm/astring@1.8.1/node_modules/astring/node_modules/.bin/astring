#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/astring@1.8.1/node_modules/astring/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/astring@1.8.1/node_modules/astring/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/astring@1.8.1/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/astring@1.8.1/node_modules/astring/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/astring@1.8.1/node_modules/astring/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/astring@1.8.1/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/astring" "$@"
else
  exec node  "$basedir/../../bin/astring" "$@"
fi
