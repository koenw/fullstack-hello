#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules/node-gyp-build/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/msgpackr-extract@1.0.15/node_modules/msgpackr-extract/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/msgpackr-extract@1.0.15/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules/node-gyp-build/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/msgpackr-extract@1.0.15/node_modules/msgpackr-extract/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/msgpackr-extract@1.0.15/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp-build@4.3.0/node_modules/node-gyp-build/build-test.js" "$@"
else
  exec node  "$basedir/../../../../../node-gyp-build@4.3.0/node_modules/node-gyp-build/build-test.js" "$@"
fi
