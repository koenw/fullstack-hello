#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@6.3.0/node_modules/semver/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@6.3.0/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/eslint-plugin-react@7.27.1_eslint@8.3.0/node_modules/eslint-plugin-react/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/eslint-plugin-react@7.27.1_eslint@8.3.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@6.3.0/node_modules/semver/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/semver@6.3.0/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/eslint-plugin-react@7.27.1_eslint@8.3.0/node_modules/eslint-plugin-react/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/eslint-plugin-react@7.27.1_eslint@8.3.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@6.3.0/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@6.3.0/node_modules/semver/bin/semver.js" "$@"
fi
