#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/which@1.3.1/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/cross-spawn@6.0.5/node_modules/cross-spawn/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/cross-spawn@6.0.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/which@1.3.1/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules:/home/koen/dev/fullstack-hello/node_modules:/home/koen/dev/node_modules:/home/koen/node_modules:/home/node_modules:/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/cross-spawn@6.0.5/node_modules/cross-spawn/node_modules:/home/koen/dev/fullstack-hello/frontend/node_modules/.pnpm/cross-spawn@6.0.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../which@1.3.1/node_modules/which/bin/which" "$@"
else
  exec node  "$basedir/../../../../../which@1.3.1/node_modules/which/bin/which" "$@"
fi
